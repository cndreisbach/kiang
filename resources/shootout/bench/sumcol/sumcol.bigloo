(module sumcol
	(main main)
	(option (set! *genericity* #f)))

(define (main args)
  (let ((sum 0))
    (let loop ((line (read-line)))
      (cond ((eof-object? line) #f)
	    (else
	     (set! sum (+ sum (string->integer line)))
	     (loop (read-line)))))
    (display sum) (newline)))
